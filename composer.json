{
    "name": "kenmaclord/potassium",
    "description": "A base admin preset",
    "keywords": ["preset", "admin interface"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "laravel/framework": "~6.0",
        "intervention/image": "^2.4",
        "mcamara/laravel-localization": "^1.4",
        "caouecs/laravel-lang": "~5.0"
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "~4.0",
        "fzaninotto/faker": "~1.9",
        "mockery/mockery": "~1.3",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Potassium\\": "src/framework",
        },
        "files": ["vendor/kenmaclord/potassium/src/framework/App/Helpers/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/utilities/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Potassium\\PotassiumServiceProvider",
                "Potassium\\App\\Providers\\EventServiceProvider",
                "Potassium\\App\\Providers\\ConfigServiceProvider",
                "Potassium\\App\\Providers\\AuthServiceProvider"
            ]
        }
    }
}
