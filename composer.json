{
    "name": "kenmaclord/potassium",
    "description": "Simple base admin for my projects",
    "license": "MIT",
    "authors": [
        {
            "name": "kenmaclord",
            "email": "karl.morisset.register@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "intervention/image": "^2.5",
        "mcamara/laravel-localization": "^1.5",
        "caouecs/laravel-lang": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Potassium\\": "src"
        },
        "files": [
            "src/App/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Potassium\\Tests\\": "src/tests/"
        },
        "files": [
            "src/tests/utilities/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Potassium\\PotassiumServiceProvider"
            ]
        }
    }
}
