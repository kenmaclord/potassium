{
    "name": "kenmaclord/potassium",
    "description": "Simple admin for my projects",
    "keywords": ["laravel", "php", "admin", "vuejs"],
    "type":"project",
    "homepage": "https://github.com/kenmaclord/potassium",
    "license": "MIT",
    "authors": [
        {
            "name": "kenmaclord",
            "email": "karl.morisset.register@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "laravel/framework": "^7.*",
        "intervention/image": "^2.5",
        "mcamara/laravel-localization": "^1.5",
        "laravel/ui":"^2.4",
        "laravel-lang/lang":"~7.0"
    },
    "autoload": {
        "psr-4": {
            "Potassium\\": "src"
        },
        "files": [
            "src/App/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "files": [
            "src/Tests/utilities/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Potassium\\PotassiumServiceProvider"
            ]
        }
    }
}
